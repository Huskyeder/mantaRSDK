\name{mantaJob.failures}
\alias{mantaJob.failures}
\title{mantaJob.failures returns list of failures given Manta job identifier}
\usage{
  mantaJob.failures(jobid)
}
\arguments{
  \item{jobid}{character required. Manta job identifier
  such as "70c30bab-873b-66da-ebc8-ced12bd35ac4" or use
  mantaJob.last() to fetch the jobid of the last manta Job
  run on the service e.g. mantaJob.errors(mantaJob.last())}
}
\description{
  mantaJob.failures returns list of failures given Manta
  job identifier
}
\keyword{Manta,}
\keyword{manta}

